{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [ "node" ],
    "lib": [ "es6" ],
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "baseUrl": "./",
    "typeRoots": [
      "./typings",
      "./node_modules/@types"
    ],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": [
    "typings/index.d.ts"
  ]
}
